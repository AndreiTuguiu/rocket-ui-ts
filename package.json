{
  "name": "rocket-ui",
  "private": true,
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "run clean && tsc -p tsconfig.build.json",
    "build-storybook": "storybook build",
    "clean": "rimraf dist",
    "postinstall": "yarn dlx sort-package-json",
    "lint": "eslint src/*.ts",
    "linter": "eslint . --fix --ext .ts,.tsx",
    "storybook": "storybook dev -p 6006",
    "test": "jest --collectCoverage --passWithNoTests--silent=false --runInBand"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testEnvironment": "jsdom",
    "verbose": true
  },
  "dependencies": {
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.124",
    "@mui/material": "^5.11.14",
    "@mui/system": "^5.11.14",
    "@mui/types": "^7.2.3",
    "classnames": "^2.3.2",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@changesets/cli": "^2.26.1",
    "@nrwl/js": "^15.8.9",
    "@storybook/addon-essentials": "7.0.0-rc.8",
    "@storybook/addon-interactions": "7.0.0-rc.8",
    "@storybook/addon-links": "7.0.0-rc.8",
    "@storybook/blocks": "7.0.0-rc.8",
    "@storybook/react": "7.0.0-rc.8",
    "@storybook/react-webpack5": "7.0.0-rc.8",
    "@storybook/testing-library": "0.0.14-next.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/babel__core": "^7",
    "@types/eslint": "^8",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.10",
    "@types/prop-types": "^15",
    "@types/ramda": "^0",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "prop-types": "15.8.1",
    "rimraf": "^4.4.1",
    "sort-package-json": "^2.4.1",
    "storybook": "7.0.0-rc.8",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "packageManager": "yarn@3.5.0",
  "engines": {
    "node": ">=18.x"
  }
}
